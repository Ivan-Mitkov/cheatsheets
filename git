git tuto: https://www.youtube.com/watch?v=7orfDC1ALvs
git config --global user.email username@host.com
git config --local uesr.email username @host.com
git config --list	//shows all config
cat ~/.gitconfig	//global config
cat .git/config		//local config
git init
git remote add origin [SSH]
git remote -v	// shows remote repositories
git pull origin master	//first time pull - links origin(remote) with master(local)
git add [filename]
git status
git commit -m "[message]"
git push --set-upstream origin master	//first time push
